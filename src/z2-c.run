reset;
option solver cplex;
option reset_initial_guesses 1;
option cplex_options 'outlev=0';
option solver_msg 0;
model src/z2.mod;
data src/z2.dat;

set in_risks_max := {5, 20, 37};

display in_risks_max;

for {m in in_risks_max} {
	let risk_max := m;
	solve >/dev/null;
	display profit, {s in Scenarios} abs(risk[s]);
}
